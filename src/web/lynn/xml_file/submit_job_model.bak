<job>
    <xml_type>homogeneous</xml_type>
    <group>common</group>
    <priority>middle</priority>
    <job_type>batch</job_type>

    <need_cpu>1</need_cpu>
    <need_memory>512</need_memory>
    <vm_type>KVM</vm_type>
    <img_template>centos_10G</img_template>
    <size>10</size>
    <app_name>app_my</app_name>

    <tasks>
        <task>
            <exe_path>getURL</exe_path>
            <task_num>1</task_num>
        </task>
        <task>
            <exe_path>getURL -b</exe_path>
            <task_num>1</task_num>
        </task>
    </tasks>

</job>
